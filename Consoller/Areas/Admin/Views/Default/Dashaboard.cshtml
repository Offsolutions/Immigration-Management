@model Consoller.Areas.Admin.Models.AdminData
@using Consoller.Areas.Auth.Models;
@using onlineportal.Areas.AdminPanel.Models;
@{
    ViewBag.Title = "Dashaboard";
    Layout = "~/Areas/Admin/Views/Shared/Layout.cshtml";
    var increment = 0;

}



<h2>DashBoard</h2>

<div class="row">
    <div class="col-xl-4">
        <div class="card overflow-hidden">
            <div id="chartContainer" style="height: 370px; width: 100%;"></div><br />
            <div id="chartContainer2" style="height: 370px; width: 100%;"></div>
        </div>
        <div class="card">
            <div class="card-body">
                <h4 class="card-title mb-4">Monthly Earning</h4>
                <div class="row">
                    <div class="col-sm-6">
                        <p class="text-muted">This month</p>
                        <h3>Rs.@Model.Sale()</h3>
                        <p class="text-muted">Today Earning</p>
                        <h3>Rs.@Model.Todayearning()</h3>

                        <div class="mt-4">
                            <a href="@Url.Action("AllPayment","Default")" class="btn btn-primary waves-effect waves-light btn-sm">View More <i class="mdi mdi-arrow-right ml-1"></i></a>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="mt-4 mt-sm-0" style="position: relative;">
